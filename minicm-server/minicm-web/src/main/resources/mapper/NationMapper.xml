<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aihwaong.minicm.mapper.NationMapper">
    <resultMap id="BaseResultMap" type="com.aihwaong.minicm.model.Nation">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="name" jdbcType="CHAR" property="name"/>
    </resultMap>

    <select id="selectAllNation" resultMap="BaseResultMap">
        SELECT * FROM cm_nation
    </select>

</mapper>